
<div style="border: 1px dotted orange">    
     <%= review.text %>
     <!-- <li><%#= r.rating %> </li> -->
      <div style="text-align:right">
           <%= link_to edit_school_review_path(school_id: @school.id, id: review) do %>
          <span class="glyphicon glyphicon-pencil" style="text-align:right"></span>
           <% end %>
           <%= link_to school_review_path(school_id: @school.id, id: review), :method => :delete do %>
          <span class="glyphicon glyphicon-trash" ></span>
           <% end %>
      </div>
</div>
